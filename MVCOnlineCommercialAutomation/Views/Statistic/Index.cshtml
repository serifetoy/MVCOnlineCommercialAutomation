@*@using MVCOnlineCommercialAutomation.Models.Classes
    @model List<Statistic>*@
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<style>
    .icon {
        width: 60px;
        height: 60px;
        font-size: 50px;
        text-align: left;
    }
    .icon-text {
        white-space: nowrap;
        text-align: right;
    }
</style>


<!--
