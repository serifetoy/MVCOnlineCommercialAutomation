<!DOCTYPE html>
<html>
<head>

    <!-- Custom Theme files -->
    <link href="~/web2/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <!-- Custom Theme files -->
    <script src="~/web2/js/jquery.min.js"></script>
    <link rel="stylesheet" href="~/web2/css/font-awesome.css">
    <!-- Custom Theme files -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Metro Panel Responsive web template, Bootstrap Web Templates, Flat Web Templates, Andriod Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyErricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--webfont-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>

    <script src="js/zingchart.min.js"></script>
    <script src="js/zingchart.jquery.js"></script>
    <script src="js/jquery.easydropdown.js"></script>
    <script src="js/jquery.nicescroll.js"></script>

    <link href="css/jqvmap.css" media="screen" rel="stylesheet" type="text/css" />


    <script src="~/web2/js/easyResponsiveTabs.js" type="text/javascript"></script>

    <link href="~/web2/css/nav.css" rel="stylesheet" type="text/css" media="all" />
    <script src="~/web2/js/main.js"></script> <!-- Resource jQuery -->
    <!-- chart -->
    <script src="~/web2/js/Chart1.js"></script>
    <!-- //chart -->
</head>
<body>
    <div class="total-content">
        <div class="list_of_members">
            <div class="sales" style="background-color: #fad390">
                <div class="icon">
                    <i class="fas fa-money-bill-alt fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.v1</h3>
                    <p>Total Customer</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color: #f6b93b">
                <div class="icon">
                    <i class="fas fa-tasks fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.v2</h3>
                    <p>Product Count</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="orders" style="background-color: #fa983a">
                <div class="icon">
                    <i class="fas fa-users fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.v3</h3>
                    <p>Personnel Count</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="visitors" style="background-color: #e58e26">
                <div class="icon">
                    <i class="fas fa-sitemap fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.v4</h3>
                    <p>Category Count</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
        </div>

    </div>
    <div class="clearfix"></div>

    <div class="total-content">
        <div class="list_of_members">
            <div class="sales" style="background-color: #b8e994">
                <div class="icon">
                    <!-- Total stock icon -->
                    <i class="fas fa-cubes fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.v5</h3>
                    <p>Total Stock</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color: #78e08f">
                <div class="icon">
                    <!-- Brand count icon -->
                    <i class="fas fa-industry fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.v6</h3>
                    <p>Brand Count</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="orders" style="background-color: #38ada9">
                <div class="icon">
                    <!-- Critical level icon -->
                    <i class="fas fa-exclamation-triangle fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.v7</h3>
                    <p>Critical Level</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="visitors" style="background-color: #079992">
                <div class="icon">
                    <!-- Max priced product icon -->
                    <i class="fas fa-thumbs-up fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3 style="font-size: 2.1rem;">@ViewBag.v8</h3>
                    <p>Bestsellers</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>


    <div class="clearfix"></div>

    <div class="total-content">
        <div class="list_of_members">
            <div class="sales" style="background-color: #82ccdd">
                <div class="icon">
                    <!-- Min priced product icon -->
                    <i class="fas fa-thumbs-down fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3  style="font-size: 2.1rem;">@ViewBag.v9</h3>
                    <p>Least Sellers</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color: #60a3bc">
                <div class="icon">
                    <!-- Max brand icon -->
                    <i class="fas fa-star fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3  style="font-size: 2.1rem;">@ViewBag.v10</h3>
                    <p>Max Brand</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="orders" style="background-color: #3c6382">
                <div class="icon">
                    <!-- Refrigerator count icon -->
                    <i class="fas fa-snowflake fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.v11</h3>
                    <p>Fridge Count</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="visitors" style="background-color: #0a3d62">
                <div class="icon">
                    <!-- Laptop count icon -->
                    <i class="fas fa-laptop fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.v12</h3>
                    <p>Laptop Count</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
        </div>

    </div>

    <div class="clearfix"></div>

    <div class="total-content">
        <div class="list_of_members">
            <div class="sales" style="background-color: #6a89cc">
                <div class="icon">
                    <i class="fas fa-chart-line fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3  style="font-size: 2.1rem;">@ViewBag.d13</h3>
                    <p>Top Seller</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color: #4a69bd">
                <div class="icon">
                    <i class="fas fa-money-bill-wave fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.v14</h3>
                    <p>Cash Amount</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="orders" style="background-color: #1e3799">
                <div class="icon">
                    <i class="fas fa-shopping-cart fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.d15</h3>
                    <p>Today's Sales</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="visitors" style="background-color: #0c2461">
                <div class="icon">
                    <i class="fas fa-cash-register fa-inverse"></i>
                </div>
                <div class="icon-text">
                    <h3>@ViewBag.v16</h3>
                    <p>Today's Cash</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</body>
</html>



